{
  "name": "VS-Snazzy",
  "type": "dark",
  "colors": {
    "editor.background": "#282a36",
    "editor.foreground": "#f1f1f0",
    "activityBarBadge.background": "#282a36",
    "sideBarTitle.foreground": "#f1f1f0",
    "sideBar.background": "#373a4b",
    "sideBarTitle.foreground": "#f1f1f0"
  },
  "tokenColors": [
    {
      "name": "Strong",
      "scope": "strong, markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment, string.quoted.docstring",
      "settings": {
        "foreground": "#616263",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f2f99d"
      }
    },
    {
      // undefined
      "scope": "storage.type.function.arrow",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Type",
      "scope": "storage.type.primitive, support.type, entity.name.type, keyword.type, keyword.other.unit",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "String",
      "scope": "string, meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, support.constant",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Undefined",
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Function call",
      "scope":
        "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super, keyword.other.get, keyword.other.set",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Identifier",
      "scope":
        "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "Constants",
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class entity.name.type.module",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9afeed"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "Keyword",
      "scope":
        "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this, variable.language.special.self",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Storage Modifier [e.g. class, extends, static] (JS)",
      "scope": "storage.modifier, storage.type.class.js, variable.language.this, variable.language.this.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "Diff insertion",
      "scope": "markup.inserted.diff, meta.diff.header.to-file",
      "settings": {
        "foreground": "#5af78e",
        "background": "#616263"
      }
    },
    {
      "name": "Diff deletion",
      "scope": "markup.deleted.diff, meta.diff.header.from-file",
      "settings": {
        "foreground": "#f37470",
        "background": "#616263"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "C# method call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#616263"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "CSS property",
      "scope": "source.css support.type.property-name",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f2f99d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML tag expression",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Java package & import",
      "scope": "source.java storage.modifier.package, source.java storage.modifier.import",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "Java annotation",
      "scope": "source.java storage.type.annotation",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Java method call",
      "scope": "source.java meta.method-call meta.method",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Java inner-class variable",
      "scope": "source.java meta.method-call meta.inner-class meta.method",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "Java inner-class method call",
      "scope": "source.java meta.method-call meta.inner-class meta.method meta.method-call meta.method",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Java inner-class method",
      "scope": "source.java meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Java null",
      "scope": "source.java constant.language",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Async",
      "scope": "source.js storage.modifier.async",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#58c6ff"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference, markup.underline.link.image",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f2f99d"
      }
    },
    {
      "name": "Markdown code",
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#f37470",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Python import *",
      "scope": "source.python keyword.operator.arithmetic",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "Python def",
      "scope": "source.python meta.function storage.type.function",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Python function arguments",
      "scope": "source.python meta.function-call.arguments",
      "settings": {
        "foreground": "#f1f1f0"
      }
    },
    {
      "name": "Python function decorators",
      "scope": "source.python meta.function entity.name.function.decorator",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "Python True & False & None",
      "scope": "source.python constant.language",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "Javascript import [e.g. import, export] (JS)",
      "scope": "keyword.control.module.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JavaScript expression",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#f37470"
      }
    },
    {
      "name": "JavaScript Property",
      "scope": "variable.other.property.js support.variable.property.dom",
      "settings": {
        "foreground": "#59f78e"
      }
    },
    {
      "name": "String Template",
      "scope": "string.template",
      "settings": {
        "foreground": "#9afeed"
      }
    },
    {
      "name": "JSX tags",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#9aedfe"
      }
    }
  ]
}
